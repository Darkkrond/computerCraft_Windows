Local function str_ro_array( str )
    Local dlina = string.len( str )
    Local result = {}
    For i = 1, dlina do
        result[i] = string.char(string.byte(str, i, i))
    end
    return result
end

Local s = fs.open("/system/apps/explorer/www-page.sys", "r")
